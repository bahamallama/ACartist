<div class="footerLinks row center">
	<div <% if user_signed_in? %> class="col-sm-3" <% else %> class="col-sm-4" <% end %>>
		<div class="footerScript text-center">
			Cover Art
		</div>
		<div class="footerLinksText text-center">
			<%= link_to "Cover", root_path, class: "btn btn-link" %>
		</div>
	</div>
  	<div <% if user_signed_in? %> class="col-sm-3" <% else %> class="col-sm-4" <% end %>>
		<div class="footerScript text-center">
			Gallery Images
		</div>
		<div class="footerLinksText text-center">
			<%= link_to "Gallery", galleries_path, class:"btn btn-link" %>
		</div>
  	</div>
  	<div <% if user_signed_in? %> class="col-sm-3" <% else %> class="col-sm-4" <% end %>>
		<div class="footerScript text-center">
			<% if user_signed_in? %>
				Sign Out
			<% else %>
				Sign In
			<% end %>
		</div>
		<div class="footerLinksText text-center">
			<% if user_signed_in? %>
				<%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: "btn btn-link" %>
			<% else %>
				<%= link_to "Sign In", new_user_session_path, class: "btn btn-link"%>
			<% end %>
		</div>
	</div>
	<% if user_signed_in? %>
		<div class="col-sm-3">
			<div class="footerScript text-center">
				Edit Profile
			</div>
			<div class="footerLinksText text-center">
				<%= link_to "Edit Profile", edit_user_registration_path, class: "btn btn-link" %>
			</div>
		</div>
	<% end %>		
</div>
